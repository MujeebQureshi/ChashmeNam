<?xml version='1.0' encoding='utf-8'?>
<widget id="inc.gorby.chashmenam" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Chashm-e-Nam</name>
    <description>
        Short description mate.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Gorby Inc.
    </author>
    <content src="starter.html" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="browser" spec="^5.0.1" />
    <engine name="android" spec="^6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-googleplus" spec="^5.1.1">
        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.478142718545-63efvhu0p7h9saeg950mmaj8v0q5rqia" />
    </plugin>
    <plugin name="cordova-plugin-facebook4" spec="^1.9.1">
        <variable name="APP_ID" value="135689240448048" />
        <variable name="APP_NAME" value="ChashmeNam" />
    </plugin>
</widget>
